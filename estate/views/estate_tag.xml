<odoo>
    <record id="tag_view_tree" model="ir.ui.view">
        <field name="name">tag.view.tree</field>
        <field name="model">estate.tag</field>
        <field name="arch" type="xml">
            <tree string="te" editable='bottom'>
                <field name="name"/>   
            </tree>
        </field>
    </record>
    <record id="tag_view_form" model="ir.ui.view">
        <field name="name">tag.view.form</field>
        <field name="model">estate.tag</field>
        <field name="arch" type="xml">
            <form string="Te1">
                <sheet>
                    <group>
                        <field name="name" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id ="tag_action" model='ir.actions.act_window'>
        <field name='name'>Property Type</field>
        <field name='res_model'>estate.tag</field>
        <field name='view_mode'>tree,form</field>
    </record>
</odoo>